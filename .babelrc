{
    "presets": [
        "babel-preset-stage-1",
        [
            "env",
               {
                "targets": {
                        "browsers": [
                            "last 2 versions",
                            "safari >= 7"
                        ]
                    }
                }
        ]
    ],
"plugins": [
    [
        "transform-react-jsx",
        {
            "pragma": "h"
        }
    ],
    [
      "transform-react-remove-prop-types", {
        "ignoreFilenames": ["node_modules"]
      }
    ]
]
}
